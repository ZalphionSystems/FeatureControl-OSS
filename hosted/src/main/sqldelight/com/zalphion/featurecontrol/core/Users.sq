get:
SELECT *
FROM core.users
WHERE user_id = ?;

getAll:
SELECT *
FROM core.users
WHERE user_id IN ?;

getByEmail:
SELECT *
FROM core.users
WHERE email_address = ?;

upsert:
INSERT INTO core.users
    (user_id, user_name, email_address, photo_url)
    VALUES (:userId, :username, :emailAddress, :photoUrl)
ON CONFLICT (user_id)
DO UPDATE SET
    user_name = :username,
    photo_url = :photoUrl;